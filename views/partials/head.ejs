<meta charset="UTF-8">
<title>Master Class</title>


<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Hanuman:wght@100;400;900&family=Lato:wght@300;400;700&display=swap" rel="stylesheet">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.min.css">
<style>
    * {
        padding: 0;
        margin: 0;
        background-color: rgba(30,30,30,255);
        /* overflow-x: hidden; */
    }

    /* Font */
    .font-lato-regualar {
        font-family: "Lato", sans-serif;
        font-weight: 400;
    }

    /* Color */

    .bg-black {
        background-color: rgba(30,30,30,255);
    }

    .text-master {
        color: rgba(239,69,98,255)!important;
    }

    /* Header CSS */
    .navbar-brand {
        max-width: 100px;
    }

    .navbar-brand > img {
        width: 100%;
        height: auto;
    }

    .nav-item > a {
        font-size: 18px;
    }

    /* Loader */
    #loader {
        position: absolute;
        width: 100%;
        height: 100vh;
        z-index: 99999;
        background: #fff;
        display: block;
    }

    .wrapper {
        position: fixed;
        top: 50%;
        right: 50%;
        transform: translate(50%,-50%);
        background-color: #fff;
        display: block;
    }

    #loader span {
        display: inline-block;
        margin-top: 10px;
        height: 20px;
        width: 20px;
        border-radius: 50%;
    }

    #loader span:not(:first-child) {
        margin-left: 10px;
    }

    #loader span:nth-child(1) {
        background: #333;
        animation: move .8s ease-in-out -0.25s infinite alternate;
    }
    #loader span:nth-child(2) {
        background: #333;
        animation: move 0.8s ease-in-out -0.5s infinite alternate;
    }
    #loader span:nth-child(3) {
        background: #333;
        animation: move 0.8s ease-in-out -0.75s infinite alternate;
    }
    #loader span:nth-child(4) {
        background: #333;
        animation: move .8s ease-in-out -1s infinite alternate;
    }

    @keyframes move {
        from {
            transform: translateY(-10px);
        }
        to {
            transform: translateY(5px);
        }
    }

    #animation-zoom-out {
        position: relative;
        -webkit-animation-name: animatezoomout;
        -webkit-animation-duration: 3s;
        animation-name: animatezoomout;
        animation-duration: 3s;
    }
    
    @-webkit-keyframes animatezoomout {
        from {
            transform: scale(2);
            opacity:0;
        }
        to {
            transition: all .1s ease-in-out;
            opacity:1;
        }
    }
    
    @keyframes animatezoomout {
        from{
            transform: scale(2);
            opacity:0;
        }
        to{
            transition: all .2s ease-in-out;
            opacity:1;
        }
    }

    #container {
        margin: auto;
        top: 0;
        bottom: 0;
        filter: url(#threshold) blur(0.6px);
    }

    #text1, #text2 {
        position: absolute;
        width: 100%;
        display: inline-block;
        font-family: 'Raleway', sans-serif;
        font-size: 80pt;
        text-align: center;
        user-select: none;
        color: rgba(239,69,98,255)!important;
    }

    .modal.show {
        background-color: rgba(0,0,0,0.6);
    }

    video {
        opacity: 0.5;
    }

    .card-img-overlay {
        background-color: rgba(0,0,0,.6);
        transition: 0.3s;
        cursor: pointer;
    }

    .card-img-overlay:hover {
        background-color: transparent;
        transition: 0.3s;
    }

    .card-img-overlay  p,
    .card-img-overlay h5 {
        background-color: transparent;
    }

    .description{
        position: absolute;
        bottom: 20px;
        background-color: transparent;
    }

    #myModal .modal-dialog {
        max-width: 1200px;
    }
    
    .profile a.active {
        background-color: rgba(239,69,98,255);
        color: white;
    }

    .profile a {
        color: rgba(239,69,98,255);
    }
    .card.profile {
        border: none;
        max-width: 500px;
    }

    @media only screen and (min-width: 768px){
        .card {
            width: 70%;
        }
    }
</style>